{
  "name": "courseai-backend",
  "version": "0.1.0",
  "description": "AI-powered personalized course platform backend",
  "main": "src/index.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "tsx src/db/migrate.ts",
    "test": "jest",
    "setup:database": "tsx scripts/setup-supabase-database.ts",
    "check:database": "tsx scripts/check-database.ts",
    "test:connection": "tsx scripts/test-supabase-connection.ts",
    "test:smoke": "tsx scripts/smoke-test.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@fastify/cors": "^8.4.2",
    "@fastify/jwt": "^7.2.4",
    "@fastify/rate-limit": "^9.1.0",
    "@supabase/supabase-js": "^2.51.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.1",
    "fastify": "^4.25.2",
    "openai": "^4.24.1",
    "pg": "^8.16.3",
    "postgres": "^3.4.3",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "drizzle-kit": "^0.20.7",
    "eslint": "^9.31.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "ai",
    "education",
    "personalized-learning"
  ],
  "author": "Courses AI Team",
  "license": "MIT"
}
