# Preview Environment Configuration
# This file is used for Vercel preview deployments

# Enable preview-specific features
ENABLE_PREVIEW_FEATURES=true
NODE_ENV=preview

# Admin token for seeding demo data
# Set this in Vercel dashboard as a secret
ADMIN_TOKEN=@admin_token

# Feature flags for preview
ENABLE_FUNCTION_CALLING=true
ENABLE_STRUCTURED_LOGGING=true

# Preview-specific settings
JWT_EXPIRES_IN=7d
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Monitoring
ENABLE_QUALITY_MONITORING=true
ENABLE_ACTION_LOGGING=true